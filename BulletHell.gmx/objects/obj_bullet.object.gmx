   obj_dmg_info.crit = true;
   dmg=obj_stat.bullet[obj_stat.stat[3],1]*obj_stat.bullet[obj_stat.stat[3],8]*(1+obj_stat.dmg_upgraded); //Dmg + crit
}else{
   dmg=obj_stat.bullet[obj_stat.stat[3],1]*(1+obj_stat.dmg_upgraded); //Nimmt den dmg wert für die ausgewählte waffe
}

//-------------
hspeed=9;
image_angle=obj_player.image_angle;

image_xscale = image_xscale + image_xscale*obj_stat.bullet[obj_stat.stat[3],5];
image_yscale = image_yscale + image_yscale*obj_stat.bullet[obj_stat.stat[3],5];

//-------------

if (obj_stat.stat[3]==0) { 
   if (obj_stat.bullet[obj_stat.stat[3],0] &gt; 0){
      direction=image_angle+random_range(90+obj_player.bullet[obj_stat.stat[3]]-obj_player.bullet[obj_stat.stat[3]]/obj_stat.bullet[0,0],90-obj_player.bullet[obj_stat.stat[3]]+obj_player.bullet[obj_stat.stat[3]]/obj_stat.bullet[0,0]);
    }else{
      direction=image_angle+random_range(90+obj_player.bullet[obj_stat.stat[3]],90-obj_player.bullet[obj_stat.stat[3]]);
    }
}
else if (obj_stat.stat[3]==1) {
     if (obj_stat.bullet[obj_stat.stat[3],0] &gt; 0){
        direction=image_angle+random_range(90+obj_player.bullet[obj_stat.stat[3]]-obj_player.bullet[obj_stat.stat[3]]/obj_stat.bullet[1,0],90-obj_player.bullet[obj_stat.stat[3]]+obj_player.bullet[obj_stat.stat[3]]/obj_stat.bullet[1,0]);
    }else{
        direction=image_angle+random_range(90+obj_player.bullet[obj_stat.stat[3]],90-obj_player.bullet[obj_stat.stat[3]]);
    }
}
else if (obj_stat.stat[3]==2) {
    with (obj_player) {
        if distance_to_point(mouse_x,mouse_y)&lt;300 {
           if (obj_stat.bullet[obj_stat.stat[3],0] &gt; 0){
              other.direction=image_angle+random_range(90+obj_player.bullet[obj_stat.stat[3]]-obj_player.bullet[obj_stat.stat[3]]/obj_stat.bullet[2,0],90-obj_player.bullet[obj_stat.stat[3]]+obj_player.bullet[obj_stat.stat[3]]/obj_stat.bullet[2,0]);
            }else{
              other.direction=image_angle+random_range(90+obj_player.bullet[obj_stat.stat[3]],90-obj_player.bullet[obj_stat.stat[3]]);
            }
        }
        else {
            other.direction=image_angle+90;
        }
    }
}
</string>
