            <kind>1</kind>
            <string>///Steuerung vom Spieler
scr_input();
//Movement
if (!dash_cd) {
    if key_left &amp;&amp; key_right hspeed=0;
    else if !key_left &amp;&amp; !key_right hspeed=0;
    if key_left hspeed=-3
    if key_right hspeed=3;
    
    if key_up &amp;&amp; key_down vspeed=0;
    else if !key_up &amp;&amp; !key_down vspeed=0;
    if key_up vspeed=-3;
    if key_down vspeed=3;
}

//Bullet wechseln
if key_scroll_up {
    if bullet&lt;2 bullet++;
    else bullet=0;
}
if key_scroll_down {
    if bullet&gt;0 bullet--;
    else bullet=2;
}

//Spielerbewegung animiert
image_angle=point_direction(x,y,mouse_x,mouse_y)-90;

//Schie√üen
if (!shoot &amp;&amp; key_shoot) {
    shoot=true;
    //Bullet 0: Pistol
    if bullet==0 {
        alarm[0]=10;
        instance_create(x,y,obj_bullet);
    }
    //Bullet 1: SMG
    else if bullet==1 {
        alarm[0]=5;
        instance_create(x,y,obj_bullet);
    }
    //Bullet 2: "Heimer W"
    else if bullet==2 {
        alarm[0]=20;
        repeat 3 instance_create(x,y,obj_bullet);
    }
    //Bullet 3: Laser
    else if bullet==3 {
        laser=1;
        if laser==1 instance_create(x,y,obj_bullet);
    }
}
if !key_shoot || bullet!=3 laser=0;

//Dash
if (dash&lt;dash_max &amp;&amp; !dash_cd &amp;&amp; key_dash) {
  dash++;
  dash_cd=true;
  shoot=true;
  alarm[0]=20;
  move_towards_point(mouse_x,mouse_y,20);
  alarm[1]=10;
  if dash==3 alarm[2]=90;
} 

//Death
if hp&lt;=0 game_restart();

</string>
