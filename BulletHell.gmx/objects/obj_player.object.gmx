        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Steuerung vom Spieler
scr_input();
//Movement
if (!dash_cd) {
    if key_left &amp;&amp; key_right hspeed=0;
    else if !key_left &amp;&amp; !key_right hspeed=0;
    if key_left hspeed=-obj_stat.stat[2];
    if key_right hspeed=obj_stat.stat[2];
    
    if key_up &amp;&amp; key_down vspeed=0;
    else if !key_up &amp;&amp; !key_down vspeed=0;
    if key_up vspeed=-obj_stat.stat[2];
    if key_down vspeed=obj_stat.stat[2];
}

//Collisions
if hspeed!=0{
   if !place_free(x+hspeed,y){
      if hspeed&gt;0 move_contact_solid(0,hspeed);
      if hspeed&lt;0 move_contact_solid(180,-hspeed);
      hspeed=0;
   }
}

if vspeed!=0{
   if !place_free(x+hspeed,y+vspeed){
      if vspeed&gt;0 move_contact_solid(270,vspeed);
      if vspeed&lt;0 move_contact_solid(90,-vspeed);
      vspeed=0;
   }
}
//Bullet wechseln
if key_scroll_up {
    if obj_stat.stat[3]&lt;3 obj_stat.stat[3]++;
    else obj_stat.stat[3]=0;
}
if key_scroll_down {
    if obj_stat.stat[3]&gt;0 obj_stat.stat[3]--;
    else obj_stat.stat[3]=3;
}

//Spielerbewegung animiert
image_angle=point_direction(x,y,mouse_x,mouse_y)-90;

//Schie√üen
if (!shoot &amp;&amp; key_shoot) {
    shoot=true;
    //Bullet 0: Pistol
    if obj_stat.stat[3]==0 {
        alarm[0]=10;
        instance_create(x,y,obj_bullet);
    }
    //Bullet 1: SMG
    else if obj_stat.stat[3]==1 {
        alarm[0]=5;
        instance_create(x,y,obj_bullet);
    }
    //Bullet 2: "Heimer W"
    else if obj_stat.stat[3]==2 {
        alarm[0]=20;
        repeat 3 instance_create(x,y,obj_bullet);
    }
    //Bullet 3: Laser
    else if obj_stat.stat[3]==3 {
        shoot=0;
        if laser==1 {
            instance_create(x,y,obj_laser);
            laser=0;
        }
    }
}

//Dash
if (dash&lt;dash_max &amp;&amp; !dash_cd &amp;&amp; key_dash) {
  dash++;
  dash_cd=true;
  shoot=true;
  alarm[0]=20;
  move_towards_point(mouse_x,mouse_y,20);
  alarm[1]=10;
  if dash==3 alarm[2]=90;
} 

//Stat screen
if (key_stats){
   draw_text(400,400,"HP:" + string(obj_stat.stat[0]))
   draw_text(400,420,"ACC_00" + string(obj_stat.bullet[0,0]))
   draw_text(400,440,"ACC_01" + string(obj_stat.bullet[1,0]))
   draw_text(400,460,"ACC_02" + string(obj_stat.bullet[2,0]))
   draw_text(400,480,"DMG_00" + string(obj_stat.bullet[0,1]))
   draw_text(400,500,"DMG_01" + string(obj_stat.bullet[1,1]))
   draw_text(400,520,"DMG_02" + string(obj_stat.bullet[2,1]))
         
}

//Death
if obj_stat.stat[1]&lt;=0 game_restart();

</string>
          </argument>
